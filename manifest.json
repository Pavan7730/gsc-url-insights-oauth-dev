{
  "manifest_version": 3,
  "name": "GSC URL Insights",
  "description": "View Google Search Console data for the current page.",
  "version": "1.0.0",
  "permissions": [
    "identity",
    "tabs"
  ],
  "host_permissions": [
    "https://www.googleapis.com/*"
  ],
  "oauth2": {
    "client_id": "669869853203-la12753n1ac5u8m5apt26fmgcnliprq0.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/webmasters.readonly"
    ]
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
